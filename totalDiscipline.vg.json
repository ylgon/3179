{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    
    "title": "Total Discipline of Summer Olympics From 1896 to 2021",
    "data": {
        "url": "https://raw.githubusercontent.com/ylgon/3179/main/summer.csv"
    },
    "hconcat":[{
    "mark": {"type": "bar", "tooltip": true},
    "width": 430,
    "encoding": {
      "x": {"field": "Discipline", "type": "nominal", "axis": {"labelAngle": 0},
      "scale": {
        "domain":["Athletics","Swimming","Boxing","Shooting","Rowing"]
      }
    },
      "y": {
        "aggregate": "count",
        "type": "quantitative"}
        
        
    }
    
  },


  {
   
