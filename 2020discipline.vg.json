{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Total Discipline of Summer Olympics 2020",
   
    "data": {
        "url": "https://raw.githubusercontent.com/ylgon/3179/main/athletes.csv"
    },
    "hconcat":[{
    "mark": {"type": "bar", "tooltip": true},
    "width": 430,
    "encoding": {
      "x": {"field": "discipline", "type": "nominal", "axis": {"labelAngle": 0},
      "scale": {
        "domain":["Athletics","Swimming","Football","Rowing","Hockey"]
